		"prismarine-entity": "^1.2.0",
		"prismarine-item": "^1.10.1",
		"prismarine-physics": "^1.3.0",
		"prismarine-recipe": "^1.1.0",
		"prismarine-viewer": "^1.19.3",
		"prismarine-windows": "^2.4.1",
